{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "jwt-decode": "^2.2.0",
    "node-gyp": "^6.1.0",
    "node-sass": "^7.0.0",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.3",
    "pusher-js": "^4.3.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-icons": "^3.9.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "redux": "^4.0.0",
